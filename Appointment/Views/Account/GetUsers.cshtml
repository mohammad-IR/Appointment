
<div class="container">
    <div class="row justify-content-center">
        <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">نام</th>
      <th scope="col">شماره تلفن</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var item in Model){
        <tr>
          <th scope="row">@item.Id</th>
          <td>@item.FirstName  @item.LastName</td>
          <td>@item.PhoneNumber</td>
        </tr>
    }
  </tbody>
</table>
    </div>
</div>